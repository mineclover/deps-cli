{
  "extends": "./tsconfig.base.json",
  "include": ["src"],
  "compilerOptions": {
    "types": ["node"],
    "rootDir": "src",
    "outDir": "dist",
    "module": "ESNext",
    "moduleResolution": "node",
    "target": "ES2022",
    "noEmit": false,
    "declaration": true,
    "sourceMap": false,
    "composite": false
  }
}