# deps-cli 환경 설정 예시
# 실제 사용 시 .env 파일로 복사하여 사용하세요

# Notion API 설정
# NOTION_API_KEY=secret_your_notion_api_key_here
# NOTION_DATABASE_ID=your_database_id_here
# NOTION_PAGE_ID=your_page_id_here
# NOTION_API_VERSION=2022-06-28

# 분석 설정
DEPS_CLI_MAX_CONCURRENCY=4
DEPS_CLI_TIMEOUT=30000
DEPS_CLI_CACHE_ENABLED=true
DEPS_CLI_CACHE_TTL=3600000

# 로깅 설정
DEPS_CLI_LOG_LEVEL=info
DEPS_CLI_LOG_FORMAT=text
DEPS_CLI_LOG_ENABLED=true

# 출력 설정
DEPS_CLI_DEFAULT_FORMAT=summary
# DEPS_CLI_DEFAULT_OUTPUT_DIR=./analysis-results
DEPS_CLI_COMPRESSION=false

# 개발 환경 설정
DEPS_CLI_VERBOSE=false
DEPS_CLI_DEBUG=false
DEPS_CLI_MOCK_API=false

# Node.js 환경
NODE_ENV=production